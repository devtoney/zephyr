 #include <dt-bindings/pinctrl/samd11d-pinctrl.h>

 &pinctrl { 
 
	 sercom1_spi_default: sercom1_spi_default {
		group1 {
			pinmux = <PA4D_SERCOM0_PAD0>;
				//  <PA6D_SERCOM0_PAD2>,
				//  <PA7D_SERCOM0_PAD3>;
		};
		status = "okay";
	 };
 
	 sercom2_uart_default: sercom2_uart_default {
		group1 {
			pinmux = <PA11D_SERCOM2_PAD3>,
			         <PA10D_SERCOM2_PAD2>;
		};
		status = "okay";
	 };

	 sercom0_i2c_default: sercom0_i2c_default {
		group1 {
			pinmux = <PA15D_SERCOM2_PAD1>,
				 <PA23D_SERCOM2_PAD1>;
		};
		 status = "okay";
	 };
 };
 